<script>
    //获取carTypeGuid
    var url = location.href
    var num = url.split("?")[1]
    var carTypeGuid = num.split("=")[1]
    $.ajax({
        url: "/Home/Maps/ParkLot/GetBillingTemplate?CarTypeGuid=" + carTypeGuid,
        dataType: 'json',
        method: 'GET',
        contentType: "application/json;charset=utf-8",
        success: function (res) {
            if (res.IsSuccess == true) {
                //请求成功判断是新增还是修改
                if (res.Result !== null) {
                    //修改
                    $("#form").attr("flag", "1")
                } else {
                    //新增
                    $("#form").attr("flag", "0")
                }
                if (res.Result !== null) {
                    if (res.Result.ChargeMode == 2) {
                        var data = JSON.parse(res.Result.TemplateJson)
                        var MonetaryUnit = data.MonetaryUnit;
                        if ($("input[name=MonetaryUnit]").eq(0).val() == MonetaryUnit) {
                            $("input[name=MonetaryUnit]").eq(0).prop("checked", true)
                            $("input[name=MonetaryUnit]").eq(1).prop("checked", false)
                            change()
                        } else if ($("input[name=MonetaryUnit]").eq(1).val() == MonetaryUnit) {
                            $("input[name=MonetaryUnit]").eq(1).prop("checked", true)
                            $("input[name=MonetaryUnit]").eq(0).prop("checked", false)
                            change()
                        }

                        $("#LeaveTimeout").val(data.LeaveTimeout)
                        $("#MaxDayMoney").val(data.MaxDayMoney)
                        $("#FreeMiniteAll").val(data.FreeMiniteAll)
                        if (data.FreeMiniteAll == "0") {
                            $("#FreeMiniteAll").prop("checked", true)
                        } else if(!data.FreeMiniteAll){
                            $("#FreeMiniteAll").prop("checked", false)
                        }
                        $("#BeginTime1").val(data.BeginTime1)
                        $("#EndTime1").val(data.EndTime1)
                        $("#BeginTime2").val(data.BeginTime2)
                        $("#EndTime2").val(data.EndTime2)
                        $("#BeginTime3").val(data.BeginTime3)
                        $("#EndTime3").val(data.EndTime3)
                        $("#FreeMinute1").val(data.FreeMinute1)
                        $("#FreeMinute2").val(data.FreeMinute2)
                        $("#FreeMinute3").val(data.FreeMinute3)
                        $("#Fee1").val(data.Fee1)
                        $("#Fee2").val(data.Fee2)
                        $("#Fee3").val(data.Fee3)
                        $("#SameTimeFreeMinute").val(data.SameTimeFreeMinute)
                        $("#SameTimeMoney1").val(data.SameTimeMoney1)
                        $("#SameTimeMoney2").val(data.SameTimeMoney2)
                        $("#SameTimeMoney3").val(data.SameTimeMoney3)
                        $("#SpanFreeMinute").val(data.SpanFreeMinute)
                        $("#SpanAccount").val(data.SpanAccount)
                        if (data.SpanAccount == "0") {
                            $("#SpanAccount").prop("checked", true)
                        } else if (!data.SpanAccount) {
                            $("#SpanAccount").prop("checked", false)
                        }
                        $("#SpanNoOut").val(data.SpanNoOut)
                        $("#SpanNoOutMoney").val(data.SpanNoOutMoney)
                        $("#SpanOut").val(data.SpanOut)
                        $("#SpanOutMoney").val(data.SpanOutMoney)
                        $("#OverFlag").val(data.OverFlag)
                        $("#OverFreeMinute").val(data.OverFreeMinute)
                        $("#OverMaxValue").val(data.OverMaxValue)
                        if (data.OverMaxValue == "0") {
                            $("#OverMaxValue").prop("checked", true)
                        } else if (!data.OverMaxValue) {
                            $("#OverMaxValue").prop("checked", false)
                        }
                        $("#OverNoOut").val(data.OverNoOut)
                        $("#OverNoOutMoney").val(data.OverNoOutMoney)
                        $("#OverOut").val(data.OverOut)
                        $("#OverOutMoney").val(data.OverOutMoney)
                    }
                }
            }

        }
    })
    function change() {
        var value = $("input[name='MonetaryUnit']:checked").parent().find("span").html();
        $(".classunit").html(value);
    }
</script>
