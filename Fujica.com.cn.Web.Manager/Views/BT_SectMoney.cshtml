@{
    Layout = null;
}
@*分段计费*@
<link href="~/Content/bootstrap.css" rel="stylesheet" />
<link href="~/Content/bootstrap-timepicker.min.css" rel="stylesheet" />
<link href="~/Content/bootstrap-theme.css" rel="stylesheet" />
<link href="~/Content/font-awesome.css" rel="stylesheet" />
<script src="~/Scripts/jquery-1.10.2.min.js"></script>
<script src="~/Scripts/bootstrap.min.js"></script>
<script src="~/Scripts/bootstrap-timepicker.min.js"></script>
<script src="~/Scripts/common.js"></script>
<style>
    .lh30 {
        line-height: 30px;
    }

    .padding-left-right-0 {
        padding-left: 0px;
        padding-right: 0px;
        text-align: right;
    }

    .mb30 {
        margin-bottom: 30px;
    }

    .mb40 {
        margin-bottom: 40px;
    }

    label, select, .control-label {
        color: #6C7A8B;
    }

    .input-group-addon {
        background-color: #E8F6FF;
        border: 1px solid #E7E9ED;
        color: #6C7A8B;
    }

    input.form-control {
        border: 1px solid #E7E9ED;
        color: #0092FF;
        text-align: center;
    }

    .panel {
        border-radius: 0px;
    }
    input::-webkit-outer-spin-button,
    input::-webkit-inner-spin-button {
        -webkit-appearance: none;
    }

    input[type="number"] {
        -moz-appearance: textfield;
    }

</style>
<form id="form" method="post" action="#" onsubmit="parent.SaveBtn();return false;">
    <div style="height:0;overflow: hidden;"><input type="submit" value="submit" /></div>
    <div style="margin-left:30px;width:100%;overflow:auto;height:100vh">
        <div id="" style="margin-left:20px">
            <div class="panel panel-primary" style="margin:0px -20px;border-color: #e7e9ed;">
                <div style="color: #6C7A8B; background-color: #f3f8fb; border-color: #e7e9ed;">
                    <label style="margin-left:10px;margin-top:5px;font-size:12px">公共设置</label>
                </div>
                <div class="panel-body" style="padding-bottom:10px;padding-top:10px;">
                    <!--公共设置-->
                    <div class="row">
                        <div class="col-md-6">
                            <div class="col-md-12 form-group validateinput" style="margin-bottom:30px;">
                                <label class="col-md-3 control-label font-size-12">每天最高收费</label>
                                <div class="col-md-5 input-group">
                                    <input type="number" name="MaxDayMoney" id="MaxDayMoney" class="form-control required NumText MaxNumHaxText input-sm " min="0" max="255" value="255">
                                    <span class="input-group-addon classunit">元</span>
                                </div>
                            </div>

                            <div class="col-md-12 form-group validateinput margin-bottom-0">
                                <div style="padding-top:5px;">
                                    <div class="checker col-md-1">
